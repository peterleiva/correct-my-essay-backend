{
  "name": "correct-my-essay-backend",
  "description": "O backend de uma ferramenta colaborativa que possui toda a lógica de negócio. A ferramenta é feita para conectar estudantes que desejam estudar e praticar realização de redações, também pode ser usado para correções de outros tipos de textos fora do escopo de redações, como tradução.",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "main": "src/main.js",
  "engines": {
    "node": "14.4.0"
  },
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "babel --delete-dir-on-start --copy-files --out-dir dist/ --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src/",
    "start:dev": "nodemon --exec babel-node -- ./src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterleiva/correct-my-essay-backend.git"
  },
  "keywords": [
    "collaborative",
    "essay"
  ],
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "loglevel": "^1.6.8",
    "mongoose": "^5.9.25",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "nodemon": "^2.0.4"
  }
}
